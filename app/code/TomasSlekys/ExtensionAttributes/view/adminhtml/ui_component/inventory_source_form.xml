<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="extension_attributes" sortOrder="500" component="Magento_InventoryInStorePickupAdminUi/js/form/components/fieldset">
        <settings>
            <collapsible>true</collapsible>
            <label translate="true">Extension Attributes</label>
            <dataScope>general</dataScope>
        </settings>

        <field name="example_attribute" formElement="input">
            <settings>
                <dataScope>extension_attributes.example_attribute</dataScope>
                <dataType>string</dataType>
                <label translate="true">Example attribute</label>
            </settings>
        </field>

        <field name="other_extension_attribute" formElement="input">
            <settings>
                <dataScope>extension_attributes.other_extension_attribute</dataScope>
                <dataType>string</dataType>
                <label translate="true">Other extension attribute</label>
            </settings>
        </field>
    </fieldset>
</form>
